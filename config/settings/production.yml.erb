# Twitter OAuth Setting
twitter_key:    <%= ENV['TWITTER_KEY'].to_s %>
twitter_secret: <%= ENV['TWITTER_SECRET'].to_s %>

# Facebook OAuth Setting
facebook_app_id:     <%= ENV['FACEBOOK_APP_ID'].to_s %>
facebook_app_secret: <%= ENV['FACEBOOK_APP_SECRET'].to_s %>

# GitHub OAuth Setting
github_client_id: <%= ENV['GITHUB_CLIENT_ID'].to_s %>
github_secret:    <%= ENV['GITHUB_SECRET'].to_s %>

# Gmail Pass
mail_password: <%= ENV['MAIL_PASSWORD'].to_s %>
