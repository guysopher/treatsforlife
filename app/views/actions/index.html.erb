<h1>Listing actions</h1>

<table>
  <tr>
    <th>User</th>
    <th>User name</th>
    <th>Action</th>
    <th>Action Name</th>
    <th>Pet</th>
    <th>Pet name</th>
    <th>Object</th>
    <th>Object name</th>
    <th>T</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @actions.each do |action| %>
  <tr>
    <td><%= action.uid %></td>
    <td><%= action.uname %></td>
    <td><%= action.aid %></td>
    <td><%= action.aname %></td>
    <td><%= action.pid %></td>
    <td><%= action.pname %></td>
    <td><%= action.oid %></td>
    <td><%= action.oname %></td>
    <td><%= action.t %></td>
    <td><%= link_to 'Show', action %></td>
    <td><%= link_to 'Edit', edit_action_path(action) %></td>
    <td><%= link_to 'Destroy', action, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Action', new_action_path %>
